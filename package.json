{
    "name": "git-prune-branches",
    "version": "2.0.0",
    "engines": {
        "node": ">=20"
    },
    "description": "Delete local git branches that have been merged and removed on the remote repository",
    "main": "dist/index.js",
    "type": "module",
    "preferGlobal": true,
    "bin": {
        "git-prune-branches": "dist/index.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "tsx ./src/index.ts",
        "test": "pnpm run build && vitest",
        "test:once": "pnpm run build && vitest run",
        "lint": "tsc --noEmit && eslint . --fix && pnpm run format",
        "format": "prettier . --list-different --write",
        "check-format": "prettier . --list-different",
        "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
        "ci": "pnpm build && pnpm check-exports && pnpm test:once",
        "prepublishOnly": "pnpm run ci",
        "prepare": "husky"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/patik/git-prune-branches.git"
    },
    "author": "Craig Patik",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/patik/git-prune-branches/issues"
    },
    "homepage": "https://github.com/patik/git-prune-branches",
    "keywords": [
        "git",
        "branch",
        "clean up",
        "prune",
        "remove",
        "delete",
        "interactive",
        "confirmation",
        "cli",
        "command",
        "tool"
    ],
    "dependencies": {
        "@inquirer/prompts": "^7.8.6",
        "easy-stdout": "^0.1.0",
        "inquirer-grouped-checkbox": "^0.3.0",
        "minimist": "^1.2.8",
        "ora": "^8.2.0"
    },
    "devDependencies": {
        "@arethetypeswrong/cli": "^0.18.2",
        "@eslint/js": "^9.37.0",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.18.8",
        "@vitest/ui": "^3.2.4",
        "eslint": "^9.37.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "husky": "^9.1.7",
        "prettier": "^3.6.2",
        "strip-ansi": "7.1.0",
        "tsx": "^4.20.6",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.45.0",
        "vitest": "^3.2.4"
    },
    "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa"
}
